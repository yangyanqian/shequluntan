<view><page-head vue-id="623f6a14-1" title="{{title}}" onVueInit="__l"></page-head><view hidden="{{!(isShow)}}" class="uni-common-mt"><view class="uni-uploader"><view class="uni-uploader-head"><view class="uni-uploader-title">点击可预览选好的图片</view><view class="uni-uploader-info">{{imageList.length+"/9"}}</view></view><view class="uni-uploader-body"><view class="uni-uploader__files"><block a:for="{{imageList}}" a:for-item="image" a:for-index="index" a:key="index"><view class="uni-uploader__file position-relative"><view data-event-opts="{{[['tap',[['deleImage',[index]]]]]}}" class="deleImageIcon rounded iconfont icon-shanchu text-white flex align-center justify-center position-absolute top-0 right-0" onTap="__e"></view><image class="uni-uploader__img rounded" mode="aspectFill" src="{{image}}" data-src="{{image}}" data-event-opts="{{[['tap',[['previewImage',['$event']]]]]}}" onTap="__e"></image></view></block><view class="uni-uploader__input-box rounded"><view data-event-opts="{{[['tap',[['chooseImage',['$event']]]]]}}" class="uni-uploader__input" onTap="__e"></view></view></view></view></view></view></view>