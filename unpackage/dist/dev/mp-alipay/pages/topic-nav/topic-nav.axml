<view class="body position-relative"><scroll-view class="scroll-row border-bottom border-light position-absolute fixed-top bg-white" style="height:100rpx;" scroll-x="true" scroll-into-view="{{scrollInto}}" scroll-with-animation="{{true}}"><view class="{{(('scroll-row-item px-3 py-2')+' '+(tabIndex===i?'text-main font-md font-weight-bold':''))}}" id="{{'tab'+i}}" data-event-opts="{{[['tap',[['changeTab',[i]]]]]}}" onTap="__e" a:for="{{tabBars}}" a:for-item="item" a:for-index="i" a:key="i">{{''+item.name+''}}</view></scroll-view><swiper style="{{'padding-top:101rpx;'+('height: '+scrollHeight+'px;')}}" duration="{{150}}" current="{{tabIndex}}" data-event-opts="{{[['change',[['onSwiperChange',['$event']]]]]}}" onChange="__e"><swiper-item a:for="{{listAll}}" a:for-item="itemAll" a:for-index="i" a:key="i"><scroll-view style="{{('height: '+scrollHeight+'px;')}}" scroll-y="true" data-event-opts="{{[['scrollToLower',[['loadMore',[i]]]]]}}" onScrollToLower="__e"><block a:if="{{itemAll.tabData.length>0}}"><block a:for="{{listAll[i].tabData}}" a:for-item="item" a:for-index="index" a:key="index"><topic-list vue-id="{{'2fd7ee0a-1-'+i+'-'+index}}" item="{{item}}" index="{{index}}" onVueInit="__l"></topic-list></block><load-more hidden="{{!(itemAll.tabData.length>6)}}" vue-id="{{'2fd7ee0a-2-'+i}}" loadMoreText="{{itemAll.loadMoreText}}" onVueInit="__l"></load-more></block><block a:else><templat vue-id="{{'2fd7ee0a-3-'+i}}" onVueInit="__l"><no-thing vue-id="{{('2fd7ee0a-4-'+i)+','+('2fd7ee0a-3-'+i)}}" onVueInit="__l"></no-thing></templat></block></scroll-view></swiper-item></swiper></view>