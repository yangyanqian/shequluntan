<view><view class="flex align-center p-2"><text data-event-opts="{{[['tap',[['changeTab',[i]]]]]}}" class="{{(('flex justify-center flex-1')+' '+(tabIndex==i?'text-main font-md font-weight-bold':'text-body font'))}}" onTap="__e" a:for="{{tabBarNav}}" a:for-item="item" a:for-index="i" a:key="i">{{''+item.name+''}}<text hidden="{{!(item.num>0)}}" class="ml-1">{{item.num}}</text></text></view><swiper style="{{('height: '+scrollHeight+'px;')}}" duration="{{150}}" current="{{tabIndex}}" data-event-opts="{{[['change',[['onSwiperChange',['$event']]]]]}}" onChange="__e"><swiper-item a:for="{{listAll}}" a:for-item="itemAll" a:for-index="i" a:key="i"><scroll-view style="{{('height: '+scrollHeight+'px;')}}" scroll-y="true" data-event-opts="{{[['scrollToLower',[['loadMore',[i]]]]]}}" onScrollToLower="__e"><block a:if="{{itemAll.tabData.length>0}}"><block a:for="{{listAll[i].tabData}}" a:for-item="item" a:for-index="index" a:key="index"><user-list vue-id="{{'11e62c58-1-'+i+'-'+index}}" item="{{item}}" i="{{index}}" onVueInit="__l"></user-list></block><load-more hidden="{{!(itemAll.tabData.length>10)}}" vue-id="{{'11e62c58-2-'+i}}" loadMoreText="{{itemAll.loadMoreText}}" onVueInit="__l"></load-more></block><block a:else><templat vue-id="{{'11e62c58-3-'+i}}" onVueInit="__l"><no-thing vue-id="{{('11e62c58-4-'+i)+','+('11e62c58-3-'+i)}}" onVueInit="__l"></no-thing></templat></block></scroll-view></swiper-item></swiper></view>