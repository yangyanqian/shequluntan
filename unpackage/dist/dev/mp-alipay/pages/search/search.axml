<view><block a:if="{{searchResult.length===0}}"><view class="p-2"><view class="font-md">搜索历史</view><view class="flex flex-wrap pt-1"><view data-event-opts="{{[['tap',[['clickSearchHistory',['$0'],[[['historyArr','',i,'name']]]]]]]}}" class="border rounded font px-2 py-1 mr-2 mb-2" onTap="__e" a:for="{{historyArr}}" a:for-item="item" a:for-index="i" a:key="i">{{item.text}}</view></view></view></block><block a:else><block a:for="{{searchResult}}" a:for-item="item" a:for-index="i" a:key="i"><block a:if="{{searchType=='post'}}"><common-list vue-id="{{'50c0d8d6-1-'+i}}" item="{{item}}" index="{{i}}" onVueInit="__l"></common-list><divider vue-id="{{'50c0d8d6-2-'+i}}" onVueInit="__l"></divider></block><block a:else><block a:if="{{searchType=='topic'}}"><topic-list vue-id="{{'50c0d8d6-3-'+i}}" item="{{item}}" index="{{i}}" onVueInit="__l"></topic-list></block><block a:else><block a:if="{{searchType=='user'}}"><user-list vue-id="{{'50c0d8d6-4-'+i}}" item="{{item}}" i="{{i}}" onVueInit="__l"></user-list></block></block></block></block></block></view>