<view><uni-nav-bar vue-id="1cd30cb6-1" statusBar="{{true}}" fixed="{{true}}" shadow="{{false}}" border="{{false}}" data-event-opts="{{[['^clickRight',[['clickRight']]]]}}" onClickRight="__e" onVueInit="__l"><view class="w-100 flex align-center justify-center"><view data-event-opts="{{[['tap',[['clickNavBar',[i]]]]]}}" class="{{(('mx-2')+' '+(navBarIndex==i?'font-md font-weight-bold text-main':'font text-light-black'))}}" onTap="__e" a:for="{{navBarArr}}" a:for-item="item" a:for-index="i" a:key="i">{{''+item+''}}</view></view><text class="iconfont icon-fatie_icon font-lg" slot="right"></text></uni-nav-bar><swiper style="{{('height: '+scrollHeight+'px;')}}" duration="{{150}}" current="{{navBarIndex}}" data-event-opts="{{[['change',[['onSwiperChange',['$event']]]]]}}" onChange="__e"><swiper-item><scroll-view style="{{('height: '+scrollHeight+'px;')}}" scroll-y="true" data-event-opts="{{[['scrollToLower',[['loadMore']]]]}}" onScrollToLower="__e"><block a:for="{{list.data}}" a:for-item="item" a:for-index="i" a:key="i"><common-list vue-id="{{'1cd30cb6-2-'+i}}" index="{{i}}" item="{{item}}" data-event-opts="{{[['^doSupport',[['doSupport']]]]}}" onDoSupport="__e" onVueInit="__l"></common-list><divider vue-id="{{'1cd30cb6-3-'+i}}" onVueInit="__l"></divider></block><load-more vue-id="1cd30cb6-4" loadMoreText="{{list.loadMoreText}}" onVueInit="__l"></load-more></scroll-view></swiper-item><swiper-item><scroll-view style="{{('height: '+scrollHeight+'px;')}}" scroll-y="true"><hot-cate vue-id="1cd30cb6-5" hotCateData="{{hotCateData}}" onVueInit="__l"></hot-cate><view class="p-2"><view data-event-opts="{{[['tap',[['openSearch']]]]}}" class="mb-2 flex align-center justify-center bg-light rounded" style="height:86rpx;" onTap="__e"><text class="iconfont icon-sousuo font text-light-black mr-2"></text><text class="font text-light-black">搜索话题</text></view><swiper indicator-dots="{{true}}" autoplay="{{true}}" interval="{{3000}}" duration="{{1000}}" circular="{{true}}"><swiper-item a:for="