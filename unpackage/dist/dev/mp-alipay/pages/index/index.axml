<view class="body position-relative"><scroll-view class="scroll-row border-bottom border-light position-absolute fixed-top bg-white" style="height:100rpx;" scroll-x="true" scroll-into-view="{{scrollInto}}" scroll-with-animation="{{true}}"><view class="{{(('scroll-row-item px-3 py-2')+' '+(tabIndex===i?'text-main font-md font-weight-bold':''))}}" id="{{'tab'+i}}" data-event-opts="{{[['tap',[['changeTab',[i]]]]]}}" onTap="__e" a:for="{{tabBars}}" a:for-item="item" a:for-index="i" a:key="i">{{''+item.name+''}}</view></scroll-view><swiper style="{{'padding-top:101rpx;'+('height: '+scrollHeight+'px;')}}" duration="{{150}}" current="{{tabIndex}}" data-event-opts="{{[['change',[['onSwiperChange',['$event']]]]]}}" onChange="__e"><swiper-item a:for="{{listAll}}" a:for-item="itemAll" a:for-index="i" a:key="i"><scroll-view style="{{('height: '+scrollHeight+'px;')}}" scroll-y="true" data-event-opts="{{[['scrollToLower',[['loadMore',[i]]]]]}}" onScrollToLower="__e"><block a:if="{{itemAll.tabData.length>0}}"><block a:for="{{listAll[i].tabData}}" a:for-item="item" a:for-index="index" a:key="index"><common-list vue-id="{{'8deb4120-1-'+i+'-'+index}}" item="{{item}}" index="{{index}}" data-event-opts="{{[['^follow',[['follow']]],['^doSupport',[['doSupport']]]]}}" onFollow="__e" onDoSupport="__e" onVueInit="__l"></common-list><divider vue-id="{{'8deb4120-2-'+i+'-'+index}}" onVueInit="__l"></divider></block><load-more vue-id="{{'8deb4120-3-'+i}}" loadMoreText="{{itemAll.loadMoreText}}" onVueInit="__l"></load-more></block><block a:else><templat vue-id="{{'8deb4120-4-'+i}}" onVueInit="__l"><no-thing vue-id="{{('8deb4120-5-'+i)+','+('8deb4120-4-'+i)}}" onVueInit="__l"></no-thing></templat></block></scroll-view></swiper-item></swiper></view>